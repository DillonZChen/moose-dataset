(define (domain gripper-strips_ext)
    (:requirements :derived-predicates :existential-preconditions)
    (:predicates (at ?b ?r)  (at-robby ?r)  (at__ug ?b ?r)  (at_g ?b ?r)  (ball ?b)  (carry ?o ?g)  (drop_pp ?obj ?room ?gripper)  (free ?g)  (gripper ?g)  (move_pp ?from ?to)  (pick_pp ?obj ?room ?gripper)  (room ?r))
    (:derived (at__ug ?b ?r) (and (not (at ?b ?r)) (at_g ?b ?r)))
     (:derived (drop_pp ?obj ?room ?gripper) (and (at-robby ?room) (ball ?obj) (carry ?obj ?gripper) (gripper ?gripper) (room ?room) (at__ug ?obj ?room)))
     (:derived (move_pp ?from ?to) (exists (?any_obj2 ?any_obj3) (and (at ?any_obj2 ?to) (at-robby ?from) (ball ?any_obj2) (free ?any_obj3) (gripper ?any_obj3) (room ?from) (room ?to) (at__ug ?any_obj2 ?from))))
     (:derived (move_pp ?from ?to) (exists (?any_obj2 ?any_obj3) (and (at-robby ?from) (ball ?any_obj2) (carry ?any_obj2 ?any_obj3) (gripper ?any_obj3) (room ?from) (room ?to) (at__ug ?any_obj2 ?to))))
     (:derived (pick_pp ?obj ?room ?gripper) (exists (?any_obj3) (and (at ?obj ?room) (at-robby ?room) (ball ?obj) (free ?gripper) (gripper ?gripper) (room ?room) (room ?any_obj3) (at__ug ?obj ?any_obj3))))
    (:action drop
        :parameters (?obj ?room ?gripper)
        :precondition (and (ball ?obj) (room ?room) (gripper ?gripper) (carry ?obj ?gripper) (at-robby ?room) (drop_pp ?obj ?room ?gripper))
        :effect (and (at ?obj ?room) (free ?gripper) (not (carry ?obj ?gripper)))
    )
     (:action move
        :parameters (?from ?to)
        :precondition (and (room ?from) (room ?to) (at-robby ?from) (move_pp ?from ?to))
        :effect (and (at-robby ?to) (not (at-robby ?from)))
    )
     (:action pick
        :parameters (?obj ?room ?gripper)
        :precondition (and (ball ?obj) (room ?room) (gripper ?gripper) (at ?obj ?room) (at-robby ?room) (free ?gripper) (pick_pp ?obj ?room ?gripper))
        :effect (and (carry ?obj ?gripper) (not (at ?obj ?room)) (not (free ?gripper)))
    )
)